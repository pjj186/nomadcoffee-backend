{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 10
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "./src/**/*"],
  "exclude": ["node_modules"]
}
